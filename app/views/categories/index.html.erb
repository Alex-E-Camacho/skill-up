
<table class="table">
  <% @categories.each do |c| %>
  <thead>
    <tr>
      <th><%= c.title %></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row"><%= c.id %></th>
      <% c.subcategories.each do |s| %>
        <th><%= s.title %><th>
      <% end %>
    </tr>
  </tbody>
  <% end %>
</table>
