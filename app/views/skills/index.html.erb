            <div class="card-block">
                <h2 class="card-title white-text">Technology</h2>
                <% @tech.each do |t| %>
                <p class="card-text"><h3><%= link_to t.title, t, class: "black-text" %></h3></p>
                <% end %>
            </div>
        </div>
